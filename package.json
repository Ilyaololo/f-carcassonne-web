{
  "name": "f-carcassonne-web",
  "description": "",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com:Ilyaololo/f-carcassonne-web.git"
  },
  "bugs": {
    "url": "https://github.com/Ilyaololo/f-carcassonne-web/issues"
  },
  "homepage": "https://github.com/Ilyaololo/f-carcassonne-web#readme",
  "scripts": {
    "build": "better-npm-run build",
    "dev": "concurrently \"better-npm-run dev\" \"better-npm-run serve\"",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "serve": "better-npm-run serve"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "WEBPACK_CONFIG": [
          "webpack.config.prod.ts"
        ]
      }
    },
    "dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "WEBPACK_CONFIG": [
          "webpack.config.dev.ts"
        ]
      }
    },
    "serve": {
      "command": "serve --listen $PORT public/",
      "env": {}
    }
  },
  "dependencies": {
    "class-autobind-decorator": "^3.0.1",
    "event-emitter": "^0.3.5",
    "inversify": "^5.0.1",
    "log4js": "^4.1.0",
    "mousetrap": "^1.6.3",
    "reflect-metadata": "^0.1.13",
    "three": "^0.103.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/event-emitter": "^0.3.3",
    "@types/node": "^11.13.0",
    "@types/three": "^0.93.30",
    "@types/webpack": "^4.4.27",
    "awesome-typescript-loader": "^5.2.1",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "^2.0.1",
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "dotenv-webpack": "^1.7.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^1.3.1",
    "serve": "^11.0.0",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-config": "^7.5.0"
  },
  "author": {
    "name": "Lovenkov Ilya",
    "email": "ilyaololo96@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": "10.x",
    "yarn": "1.x"
  }
}
